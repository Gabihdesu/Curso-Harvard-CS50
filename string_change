def mutate_string(string, position, character):
    # alterar a letra string na posição position pela letra character
    l = list(string)
    l[position] = character

    return print(''.join(string+l)) 
      

if __name__ == '__main__':
    s = input()
    i, c = input().split()
    s_new = mutate_string(s, int(i), c)
    print(s_new)